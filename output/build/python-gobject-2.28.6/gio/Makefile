	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --foreign gio/Makefile'; \
	$(am__cd) $(top_srcdir) && \
	  $(AUTOMAKE) --foreign gio/Makefile
