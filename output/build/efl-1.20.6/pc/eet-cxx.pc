prefix=/usr
exec_prefix=/usr
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Eet C++
Description: C++ API for the eet library
Version: 1.20.6
Requires.private: emile >= 1.20.6 eina >= 1.20.6 openssl   efl >= 1.20.6 eina >= 1.20.6 openssl   zlib >= 1.2.3  
Libs: -L${libdir} -leet
Libs.private:   -ljpeg -lm
Cflags: -I${includedir}/efl-1 -I${includedir}/eet-1 -I${includedir}/efl-1 -I${includedir}/eet-cxx-1 -I${includedir}/eet-cxx-1/eet-cxx
